#  专转 住住转
PORT=3001
NODE_ENV=development
LOG_LEVEL=info

#  转 注
WORKSPACE_DIR=./workspace
PROJECTS_ROOT=./projects
AGENTS_DIR=./agents

#  专转 住驻拽 AI
AI_PROVIDER=openai

OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4

CLAUDE_API_KEY=your_claude_api_key_here
CLAUDE_MODEL=claude-3.7-sonnet

HUGGINGFACE_API_KEY=your_huggingface_api_key_here
HUGGINGFACE_MODEL=mistralai/Mixtral-8x7B-Instruct-v0.1

#  转 转 住拽专驻
FORBIDDEN_COMMANDS=rm -rf /,sudo,chmod 777

#  专转 砖专
DASHBOARD_URL=http://localhost:3000

#  住 Git Sync
GIT_USERNAME=your_github_username
GIT_EMAIL=your_github_email
GIT_TOKEN=your_github_token
GIT_REPOSITORY=your_github_repository

# 专转 专 住
# 驻砖专转: json, sqlite (注转)
MEMORY_MODE=json

# ----- API Keys -----
OPENAI_API_KEY=your_openai_api_key_here
CLAUDE_API_KEY=your_anthropic_api_key_here
HUGGINGFACE_API_KEY=your_huggingface_api_key_here

# ----- Server -----
PORT=5001
HOST=localhost

# ----- Git -----
GIT_USERNAME=your_github_username
GIT_EMAIL=your_github_email
GIT_TOKEN=your_github_token

# ----- Project -----
DEFAULT_PROJECT_PATH=./workspace/default-project

# ----- Logs -----
LOG_LEVEL=info

# ----- Memory -----
MEMORY_MODE=json

# ----- Agent Configuration -----
# Dev Agent
DEV_AGENT_MODE=debug
USE_SUB_AGENTS=true

# Git Sync Agent
GIT_SYNC_INTERVAL=30
AUTO_COMMIT=true

# Summary Agent
SUMMARY_INTERVAL=60

# QA Agent
QA_INTERVAL=120

# Scheduler Agent
SCHEDULER_AUTO_INIT=true
